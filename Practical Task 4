    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
  }
  
  .topnav a:hover {
    background-color: #ddd;
    color: black;
  }
  
  .column {
    float: left;
    width: 33.33%;
    padding: 15px;
  }
  
